{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "src.main:app",
    "OPENAI_API_KEY": "@openai-api-key",
    "ALLOWED_ORIGINS": "@allowed-origins",
    "ENVIRONMENT": "production"
  }
}